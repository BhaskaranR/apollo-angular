{
  "name": "hello-world",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "prestart": "npm run build",
    "start": "nodemon server.js --exec babel-node",
    "clear": "rimraf dist/",
    "prebuild": "npm run clear",
    "build": "npm run build-only",
    "build-only": "./node_modules/.bin/ngc -p src/tsconfig.json",
    "test": "npm run build",
    "postbuild": "npm run tree-shaking",
    "tree-shaking": "webpack --config config/webpack.production.js",
    "preanalyze": "npm run clear && npm run build-only",
    "analyze": "webpack --config config/webpack.analyze.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.3.1",
    "@angular/compiler": "~2.3.1",
    "@angular/compiler-cli": "~2.3.1",
    "@angular/core": "~2.3.1",
    "@angular/forms": "~2.3.1",
    "@angular/http": "~2.3.1",
    "@angular/platform-browser": "~2.3.1",
    "@angular/platform-browser-dynamic": "~2.3.1",
    "@angular/platform-server": "~2.3.1",
    "@angular/router": "~3.3.1",
    "angular2-apollo": "file:../../",
    "apollo-client": "~0.5.1",
    "core-js": "~2.4.0",
    "graphql-tag": "^1.1.2",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "~0.7.2"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.30",
    "@types/node": "^6.0.38",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "casual": "^1.5.3",
    "express": "^4.14.0",
    "graphql": "~0.7.0",
    "graphql-server-express": "^0.4.3",
    "graphql-tools": "~0.8.0",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "typescript": "~2.0.6",
    "webpack": "^2.2.0-rc.1",
    "webpack-bundle-analyzer": "^2.2.0"
  }
}
